<script type="text/javascript" language="javascript">
    $(document).ready(function () {        
        $("li").on("keydown", ".li", function (key) {
            if (key.which === 13) {
                var id = this.id;
                var split_id = id.split("");
                var thisid = split_id[0];
                let text = $(`#${thisid}`).val();
                $(`#${thisid}`).attr({value : `${text}`})
                alert(text);
            }
            key.preventDefault;
        });
    });
</script>
